{
  "name": "image-analysis",
  "version": "1.0.0",
  "description": "Serverless application to do image analysis using AWS Rekognition and AWS Translate.",
  "main": "index.js",
  "scripts": {
    "build": "tsup src",
    "predeploy": "npm run build",
    "deploy": "sls deploy",
    "invoke": "sls invoke -f image-analysis",
    "preinvoke:local": "npm run build",
    "invoke:local": "sls invoke local -f image-analysis",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1251.0"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.1.3",
    "@types/aws-lambda": "^8.10.108",
    "@types/node": "^18.11.9",
    "aws-lambda": "^1.0.7",
    "eslint": "^8.27.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "jest": "^29.3.1",
    "ntl": "^5.1.0",
    "serverless": "^3.24.1",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4"
  }
}
